
@{
    Layout = null;
}
<link href="~/Content/all.css" rel="stylesheet" />
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<script src="~/Scripts/all.js"></script>
<script src="~/Scripts/bootstrap.js"></script>
<div class="card">
    <div class="card-body  text-center" style="background: radial-gradient(#ffc1072b, transparent);">
        <h1><i class="fas fa-exclamation-circle text-warning"></i></h1>
        @if (ViewBag.Message == "incorrectuser")
        {
            <h5>you don't have permission to access this ME Expert</h5>
        }
        else if (ViewBag.Message == "althenfailed")
        {
            <h5>login authentication failed</h5>
        }
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
    });
</script>