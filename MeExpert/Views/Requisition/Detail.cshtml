@using MeExpert.Models;
@using MeExpert.CommonProvide;
@{
    var a = new Management();
    List<Client> clent = a.GetClients();
    List<Position> pos = a.GetPosition();
    List<Cand> cand = a.GetCands();
    List<Cand> cand_by_req = a.GetCandByReq(ViewBag.ReqID);
    List<ContactPerson> cont_prs = a.GetPrsByClientID(ViewBag.ClientID);

    System.Threading.Tasks.Task<List<UserApi>> intwer = System.Threading.Tasks.Task.Run<List<UserApi>>(async () => await a.GetUserList());
    var usr_intwer = intwer.Result;
}
@{
    ViewBag.Title = "Requisition Detail";
}
<link href="~/Content/summernote.min.css" rel="stylesheet" />
<script src="~/Scripts/summernote.min.js"></script>
<style>
    .filter-option-inner-inner {
        color: #212529;
        font-weight: 700;
    }

    .btn-cand {
        padding: 0px 8px 0px 8px;
        font-size: .875rem !important;
    }

    a.line-cont {
        color: #007bff !important;
    }

        a.line-cont:hover {
            color: #007bff !important;
            border-bottom: 1px solid #007bff;
            cursor: pointer;
        }

    .tooltip > .tooltip-inner {
        background-color: #007bff;
        color: white;
        font-size: 12px;
    }
   
    .todo-list > li .tools {
        display: block;
        font-size: 18px;
        color: #138496;
        cursor: pointer;
    }
</style>
<div class="col-12">
    <div class="card">
        <div class="card-header">
            <div class="form-group" style="position: absolute;">
                <input type="checkbox" checked data-toggle="toggle" data-on="Open" data-off="Close" data-onstyle="warning" data-offstyle="secondary" id="status">
                <span class="status_date"></span>
            </div>
            <div class="card-tools">
                <button class="btn btn-info" id="upload-req-file"><i class="fas fa-file-upload"></i> Upload Requisition file</button>
                <button class="btn btn-primary btn-pdf-view"><i class="fas fa-file-pdf"></i> Preview Requisition</button>
                <button class="btn btn-danger" id="btn-del"><i class="fas fa-trash"></i> Delete</button>
                <button class="btn btn-success" id="btn-save"><i class="fas fa-save"></i> Save</button>
            </div>
        </div>
        <div class="card-body" style="padding-top: 0px;padding-bottom: 0px;">
            <form class="formValidate" id="formValidate" method="get">
                <div class="row">
                    <div class="col-3">
                        <div class="form-group">
                            <label>Req No.</label>
                            <input type="text" class="form-control form-control-sm  req_no" name="nreqno" style="width:100%" />
                        </div>
                    </div>
                    <div class="col-6">
                        <div class="form-group">
                            <label>Project</label>
                            <input type="text" class="form-control form-control-sm  req_title" name="ntitle" style="width:100%" />
                        </div>
                    </div>
                    <div class="col-3">
                        <div class="form-group">
                            <label>Create Date</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text"><i class="fas fa-calendar-alt text-info"></i></span>
                                </div>
                                <input type="text" class="form-control form-control-sm create_date" disabled>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-6" style="background: #fafbfc;height:100%">
                        <label>Client</label>
                        <div class="form-group">
                            <select class="selectpicker form-control client-id" data-live-search="true">
                                @foreach (Client item in clent)
                                {
                                    <option value="@item.client_id" clent-adrs="@item.client_address" clent-tel="@item.client_tel">@item.client_name</option>
                                }
                            </select>
                        </div>
                        <div class="form-group" style="color: #808285;">
                            <div class="col-12">
                                <label>Address &nbsp;</label>
                                <span class="clent-adrs"></span>
                                <label style="margin-left: 1% ">Tel.&nbsp;</label>
                                <span class="clent-tel"></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-6">
                        <div class="col-12">
                            <div class="row">
                                <div class="col-8">
                                    <div class="form-group">
                                        <label>Position</label>
                                        <select class="form-control-sm selete2-posi" style="width: 100%;color: black;font-weight: 600;">
                                            @foreach (Position item in pos)
                                            {
                                                <option value="@item.position_id">@item.position_name</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-4">
                                    <div class="form-group">
                                        <label>Headcounter</label>
                                        <input type="number" min="0" style="width:100%" class="form-control-sm headcounter" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12">
                            <label>Note</label>
                            <div class="form-group">
                                <textarea style="width: 100%;" type="text" class="form-control-sm remark box-size"></textarea>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div class="card">
        <div class="card-header">
            <div class="card-tools">
                <button class="btn btn-success" id="btn-add-cv"><i class="fas fa-plus"></i> Add Candidate</button>
            </div>
        </div>
        <div class="card-body">
            <table class="table table-sm table-hover table-bordered" style="width: 100%">
                <thead>
                    <tr>
                        <th>Action</th>
                        <th>No.</th>
                        <th>Candidate Name</th>
                        <th>Sent<span class="count-send badge badge-warning"></span></th>
                        <th>CV Return</th>
                        <th>interview 1</th>
                        <th>interview 2</th>
                        <th>interview 3</th>
                        <th>Status</th>
                        <th style="width: 26%;">Remark</th>
                    </tr>
                </thead>
                <tbody class="tb-list">
                </tbody>
            </table>
        </div>
    </div>
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">
                <strong style="color: #6c757d;"><i class="fas fa-stamp"></i>  Requisition Success</strong>
            </h3>
            <div class="card-tools">
                <button class="btn btn-primary btn-select-cand"><i class="fas fa-clipboard-check"></i> Select Candidate</button>
            </div>
        </div>
        <div class="card-body">
            <form class="formValidate" id="formValidate" method="get">
                <div class="col-12 row">
                    <div class="col-4">
                        <div class="form-group">
                            <div class="info-box">
                                <span class="info-box-icon bg-info elevation-1"><i class="fas fa-calendar-alt"></i></span>
                                <div class="info-box-content">
                                    <div class="col-12" style="position: absolute;">
                                        <span class="info-box-text" style=" margin-bottom: 2%;">Start Requisition Date</span>
                                        <input type="text" class="datepicker form-control-sm create-succ col-6" />
                                        @*  <span>&nbsp;&nbsp;-&nbsp;&nbsp;</span>*@
                                        @*<input type="text" class="datepicker form-control-sm enddate col-5" name="nenddate"/>*@
                                        <span class="info-box-number">
                                            <span><strong style="color: #6c757d;" class="dur-date"></strong></span>
                                        </span>
                                    </div>
                                </div>
                                <!-- /.info-box-content -->
                            </div>
                            <!-- /.info-box -->
                        </div>
                    </div>
                    <div class="col-8">
                        <table class="table table-sm table-hover table-bordered" style="width: 100%">
                            <thead>
                                <tr>
                                    <th>No.</th>
                                    <th>Candidate Name</th>
                                    <th>Employee ID</th>
                                    <th>Start Working  Date</th>
                                    <th style="border: 1px solid #ffffff;"></th>
                                </tr>
                            </thead>
                            <tbody class="tb-list-selected">
                            </tbody>
                        </table>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
