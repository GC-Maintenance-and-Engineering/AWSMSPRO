            //------------------------cal-date----------------------------//
            function calDateDel(s_date, f_date) {
                var f = s_date;
                var r = f_date;
                var fdate = f.split("/").reverse().join(",");
                var rdate = r.split("/").reverse().join(",");
                if (navigator.userAgent.indexOf('MSIE') !== -1
                    || navigator.appVersion.indexOf('Trident/') > -1) {

                    fdate = ConvertDate(f);
                    rdate = ConvertDate(r);
                    fdate = fdate + "T00:00:00.000Z";
                    rdate = rdate + "T00:00:00.000Z";
                }
                f = new Date(fdate);
                r = new Date(rdate);

                if (f <= r && f != "" && r != "") {
                    var totalDays = Math.round((r - f) / 8.64e7);
                    var wholeWeeks = totalDays / 7 | 0;
                    var days = wholeWeeks * 5;
                    if (totalDays % 7) {
                        f.setDate(f.getDate() + wholeWeeks * 7);

                        while (f < r) {
                            f.setDate(f.getDate() + 1);
                            if (f.getDay() != 0 && f.getDay() != 6) {
                                ++days;
                            }
                        }
                    }
                    return days;
                }

            }
            function ConvertDate(date) {
                var date = date.split("-");
                var months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
                for (var j = 0; j < months.length; j++) {
                    if (date[1] == months[j]) {
                        date[1] = months.indexOf(months[j]) + 1;
                    }
                }
                if (date[1] < 10) {
                    date[1] = '0' + date[1];
                }
                return date[2] + '-' + date[1] + '-' + date[0];
            }
