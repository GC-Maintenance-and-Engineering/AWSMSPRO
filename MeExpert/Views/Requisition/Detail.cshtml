            function autoheight() {
                $('[class*="box-size"]').each(function () {
                    var a = this;
                    if (!$(a).prop('scrollTop')) {
                        do {
                            var b = $(a).prop('scrollHeight');
                            var h = $(a).height();
                            $(a).height(h - 5);
                        }
                        while (b && (b != $(a).prop('scrollHeight')));
                    };
                    $(a).height($(a).prop('scrollHeight') + 20);
                })
            }
